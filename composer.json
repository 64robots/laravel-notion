{
    "name": "64robots/laravel-notion",
    "description": "Package to work with Notion API from Laravel",
    "keywords": [
        "64robots",
        "laravel",
        "laravel-notion",
        "notion"
    ],
    "homepage": "https://github.com/64robots/laravel-notion",
    "license": "MIT",
    "authors": [
        {
            "name": "64 Robots",
            "email": "miguel@64robots.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "../php-notion"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37",
        "64robots/php-notion": "*"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^4.4"
    },
    "autoload": {
        "psr-4": {
            "R64\\Notion\\": "src",
            "R64\\Notion\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "R64\\Notion\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "R64\\Notion\\NotionServiceProvider"
            ],
            "aliases": {
                "Notion": "R64\\Notion\\Facades\\Notion"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
